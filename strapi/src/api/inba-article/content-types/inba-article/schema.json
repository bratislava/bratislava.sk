{
  "kind": "collectionType",
  "collectionName": "inba_articles",
  "info": {
    "singularName": "inba-article",
    "pluralName": "inba-articles",
    "displayName": "in.ba články",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "title",
      "required": true
    },
    "coverImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "inbaTag": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::inba-tag.inba-tag",
      "inversedBy": "inbaArticles"
    },
    "inbaRelease": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::inba-release.inba-release",
      "inversedBy": "inbaArticles"
    },
    "perex": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "content": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    }
  }
}
