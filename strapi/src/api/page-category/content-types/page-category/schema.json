    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "shortTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "color": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "red",
        "blue",
        "green",
        "yellow",
        "purple",
        "brown"
      ]
    },
    "icon": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "mesto_01",
        "doprava_mapy_02",
        "zp_vystavba_03",
        "socialna_pomoc_04",
        "vzdelavanie_05",
        "kultura_06"
      ]
    },
    "priority": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "integer"
    },
    "pages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
