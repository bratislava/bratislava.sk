{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "metaDiscription": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "pageColor": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": ["red", "blue", "green", "yellow", "purple", "brown"]
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text"
    },
    "sections": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "sections.accordion",
        "sections.articles-list",
        "sections.calculator",
        "sections.columned-text",
        "sections.contact",
        "sections.divider",
        "sections.file-list",
        "sections.icon-title-desc",
        "sections.links",
        "sections.list-items",
        "sections.narrow-text",
        "sections.newsletter",
        "sections.numerical-list",
        "sections.text-with-image",
        "sections.videos",
        "sections.waves",
        "sections.iframe"
      ]
    },
    "pageHeaderSections": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": ["sections.featured-blog-posts", "sections.subpage-list"]
    },
    "parentPage": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "childPages"
    },
    "childPages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "parentPage"
    },
    "relatedContents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "pageBackgroundImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "pageButtonContent": {
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "blocks.blog-post-link"
    }
  }
}
