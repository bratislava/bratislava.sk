{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Str√°nky",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "path": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "alias": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subtext": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metaDiscription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "metaDescription": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "keywords": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "showTableOfContents": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": false
    },
    "pageCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page-category.page-category",
      "inversedBy": "pages"
    },
    "pageColor": {
      "type": "enumeration",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "enum": [
        "red",
        "blue",
        "green",
        "yellow",
        "purple",
        "brown",
        "starz"
      ]
    },
    "pageBackgroundImage": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "headerLinks": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "blocks.common-link",
      "repeatable": true
    },
    "pageHeaderSections": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "header-sections.event",
        "header-sections.facility",
        "sections.subpage-list"
      ],
      "max": 1
    },
    "subnavigation": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.subnavigation",
      "repeatable": false
    },
    "sections": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "sections.accordion",
        "sections.articles",
        "sections.banner",
        "sections.calculator",
        "sections.columned-text",
        "sections.columns",
        "sections.comparison-section",
        "sections.contacts-section",
        "sections.divider",
        "sections.documents",
        "sections.events",
        "sections.facilities",
        "sections.faqs",
        "sections.faq-categories",
        "sections.file-list",
        "sections.gallery",
        "sections.iframe",
        "sections.inba-articles-list",
        "sections.inba-releases",
        "sections.links",
        "sections.narrow-text",
        "sections.newsletter",
        "sections.numbers-overview",
        "sections.numerical-list",
        "sections.official-board",
        "sections.organizational-structure",
        "sections.opening-hours",
        "sections.partners",
        "sections.pros-and-cons-section",
        "sections.regulations",
        "sections.starz-landing-page",
        "sections.text-with-image",
        "sections.text-with-image-overlapped",
        "sections.tootoot-events",
        "sections.videos"
      ]
    },
    "sidebar": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "sidebars.empty-sidebar"
      ],
      "max": 1
    },
    "parentPage": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "childPages"
    },
    "childPages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "parentPage"
    },
    "parent": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "children"
    },
    "children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::page.page",
      "mappedBy": "parent"
    },
    "fullPath": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": false,
      "unique": true
    },
    "redirects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::redirect.redirect",
      "mappedBy": "page"
    },
    "breadcrumbTitle": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "relatedContents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    },
    "adminGroups": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::admin-group.admin-group",
      "inversedBy": "pages"
    }
  }
}
