{
  "name": "bratislava-next",
  "version": "1.5.0",
  "private": true,
  "license": "EUPL-1.2",
  "scripts": {
    "dev": "next dev",
    "develop": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc",
    "lint": "eslint src",
    "lint:fix": "npm run lint -- --fix",
    "lint:quiet": "npm run lint -- --quiet",
    "gen": "graphql-codegen && prettier --write src/services/graphql/index.ts",
    "prettier": "prettier --write .",
    "parse-translations": "i18next",
    "update-i18next": "npm install next-i18next@latest react-i18next@latest i18next@latest --save-exact && npm install eslint-plugin-i18next --save-dev --save-exact",
    "update-markdown": "npm install remark-directive@latest remark-directive-rehype@latest remark-gfm@latest remark-supersub@latest",
    "update-next": "npm install next@latest react@latest react-dom@latest sharp@latest --save-exact && npm install @types/react@latest @types/react-dom@latest eslint-config-next@latest typescript@latest @next/eslint-plugin-next --save-dev --save-exact",
    "update-react-aria": "npm install @internationalized/date@latest @react-stately/utils@latest react-aria@latest react-aria-components@latest react-stately@latest tailwindcss-react-aria-components --save-exact",
    "update-tailwind": "npm install tailwind-merge@latest clsx@latest --save-exact && npm install tailwindcss@latest autoprefixer@latest postcss@latest prettier-plugin-tailwindcss@latest eslint-plugin-tailwindcss@latest --save-dev --save-exact",
    "update-prettier": "npm install prettier@latest eslint-config-prettier@latest eslint-plugin-prettier@latest --save-dev --save-exact",
    "update-graphql": "npm install graphql@latest graphql-request@latest graphql-tag@latest --save-exact && npm install @graphql-codegen/cli@latest @graphql-codegen/typescript@latest @graphql-codegen/typescript-operations@latest @graphql-codegen/typescript-graphql-request@latest --save-dev --save-exact",
    "update-radix": "npm install @radix-ui/react-navigation-menu@latest --save-exact",
    "update-react-query": "npm install @tanstack/react-query@latest --save-exact && npm install @tanstack/eslint-plugin-query@latest --save-dev --save-exact",
    "update-mapbox": "npm install react-map-gl@latest mapbox-gl@latest --save-exact",
    "update-typescript": "npm install typescript@latest --save-dev --save-exact"
  },
